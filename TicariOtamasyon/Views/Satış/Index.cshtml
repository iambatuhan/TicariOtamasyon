
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<link href="~/fonts/StyleSheet1.css" rel="stylesheet" />
<table class="table table-bordered" style="margin-top:20px; margin-left:20px">
    <tr>
        <th>ID</th>
        <th>Departman Adı</th>

    </tr>
    @foreach (var k in Model)
    {

        <tr>


            <td>@k.DepartmanID</td>
            <td>@k.DepartmanAd</td>
            <td><a href="/Departman/DepartmanSil/@k.DepartmanID" class="custom-btn btn-11" style="width:110px;height:40px;text-align:center">Sil</a></td>
            <td><a href="/Departman/DepartmanGetir/@k.DepartmanID" class="custom-btn btn-10" style="width:110px;height:40px;text-align:center">Güncelle</a></td>
            <td><a href="/Departman/DepartmanDetay/@k.DepartmanID" class=" custom-btn btn-4" style="width:110px;height:40px;text-align:center">Detaylar</a></td>

        </tr>
    }
</table>
<a href="/Departman/DepartmanEkle" class="btn btn-danger">Ekle</a>
